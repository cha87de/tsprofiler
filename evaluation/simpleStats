#!/usr/bin/python
from util import readTsValues
import numpy as np

def main():
    tsvalues = readTsValues("tsinput-dailynet.csv")
    inarr = np.array(tsvalues)
    avg = np.average(inarr)
    stddev = np.std(inarr)
    min = np.min(inarr)
    max = np.max(inarr)

    print("Avg:\t%f" % avg)
    print("Std:\t%f" % stddev)
    print("Min:\t%f" % min)
    print("Max:\t%f" % max)

main()